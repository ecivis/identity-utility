        "testbox":"^2.8.0",
        "commandbox-docbox":"^1.3.2"
    },
    "ignore":[
        "**/.*",
        "tests",
        "apidocs"
    ],
    "installPaths":{
        "testbox":"testbox"
    },
    "scripts":{
        "generate-docs":"rm recurse=true force=true path=apidocs && docbox generate source=models mapping=models strategy-outputDir=apidocs strategy-projectTitle='Identity Utility'"
